<%- include("partials/header") -%>
<h1>Compose</h1>
<form class="" action="/compose" method="post">
  <div class="form-group">
    <label for="title" class="form-label">Title</label>
    <input type="text" class="form-control" name="newPostTitle">
  </div>
  <div class="form-group">
    <label for="post" class="form-label">Post</label>
    <textarea class="form-control" name="newPostContent" rows="5"></textarea>
  </div>
  <button type="submit" class="btn btn-primary" name="button">Publish</button>
</form>
<%- include("partials/footer") -%>